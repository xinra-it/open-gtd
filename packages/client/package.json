{
  "name": "@open-gtd/client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.10.7",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "react-scripts-ts-antd": "2.17.0"
  },
  "scripts": {
    "start": "react-scripts-ts-antd start",
    "build": "react-scripts-ts-antd build",
    "test": "react-scripts-ts-antd test --env=jsdom --coverage",
    "eject": "react-scripts-ts-antd eject",
    "lint:tslint": "tslint --project . --config tslint.prod.json",
    "fix:tslint": "tslint --project . --fix",
    "lint:prettier": "prettier-check src/**/*",
    "fix:prettier": "prettier --write src/**/*",
    "lint": "npm-run-all lint:**",
    "fix": "npm-run-all fix:**",
    "verify": "npm-run-all lint test"
  },
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.5",
    "@types/react": "^16.7.3",
    "@types/react-dom": "^16.0.9",
    "typescript": "^3.1.6",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.15.2",
    "prettier-check": "^2.0.0"
  }
}
